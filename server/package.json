  "scripts": {
    "build": "tsc",
    "start": "node build",
    "watch": "parallel tsc-watch nodemon",
    "tsc-watch": "tsc --watch",
    "nodemon": "sleep 5 && nodemon build",
    "knex": "knex --knexfile src/knexfile.ts",
