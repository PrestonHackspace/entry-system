    "@types/express": "^4.0.37",
    "@types/fs-extra-promise": "^1.0.2",
    "@types/knex": "^0.0.62",
    "@types/lodash": "^4.14.105",
    "@types/pg": "^7.1.4",
    "@types/react": "^16.0.40",
